{
  "name": "oh-sansi-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npx prisma generate",
    "start": "node index.js",
    "vercel-build": "pnpm run build && pnpm run prisma:generate && pnpm run migrate:deploy",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:dev": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "keywords": [
    "oh-sansi",
    "backend",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "Fabian Marco",
  "license": "ISC",
  "description": "Backend para sistema OH SANSI - GestiÃ³n de competencias estudiantiles",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}