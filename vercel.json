{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "installCommand": "pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm run vercel-build",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  }
}